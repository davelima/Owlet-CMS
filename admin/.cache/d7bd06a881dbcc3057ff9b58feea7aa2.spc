  <title>PHP 5.5.16 is released</title>
  <id>http://php.net/archive/2014.php#id2014-08-22-1</id>
  <published>2014-08-22T07:59:58+00:00</published>
  <updated>2014-08-22T07:59:58+00:00</updated>
  <category term="frontpage" label="PHP.net frontpage news"/>
  <category term="releases" label="New PHP release"/>
